<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xd.zt.mapper.dataManage.DataManageMapper">



    <select id="selectDataList" resultType="DataManage" >
        SELECT * FROM datamodel_source;
    </select>

    <select id="selectDataList1" resultType="DataManage" >
        SELECT * FROM datamodel_info;
    </select>
    <select id="selectDataList2" resultType="DataManage" >
        SELECT * FROM analyse_csv;
    </select>


    <!--模糊查询-->
    <select id="moHuDataList" parameterType="String" resultType="DataManage">
     SELECT * FROM datamodel_source WHERE sourcename LIKE '%${res}%'
    </select>

    <select id="moHuDataList1" parameterType="String" resultType="DataManage">
     SELECT * FROM datamodel_info WHERE dataresultname LIKE '%${res}%'
    </select>

    <select id="moHuDataList2" parameterType="String" resultType="DataManage">
     SELECT * FROM analyse_csv WHERE csvname LIKE '%${res}%'
    </select>


    <select id="selectMysqlDataList" resultType="MysqlData">
        SELECT * FROM mysqldata;
    </select>

    <delete id="MysqlDataDelete" parameterType="Integer" >
        delete from mysqldata where mysqldataid = #{modelid}
    </delete>


    <insert id="insertData" parameterType="DatamodelSource">
       INSERT into datamodel_source(modeid,sourcename,sourcepath,status,sourcetime) VALUES(#{modeid},#{sourcename},#{sourcepath},0,#{sourcetime})
    </insert>

    <insert id="insertData1" parameterType="AnalyseSource">
       INSERT into analyse_source(analysemodelid,analysesourcename,analysesourcepath,analysesourcetime) VALUES(#{modeid},#{sourcename},#{sourcepath},#{sourcetime})
    </insert>


    <insert id="insertData2" parameterType="ExperimentData">
       INSERT into experiment_data(experimentid,dataname,datapath,datatime) VALUES(#{modeid},#{sourcename},#{sourcepath},#{sourcetime})
    </insert>

    <select id="mysqlData" resultType="MysqlData" parameterType ="java.lang.Integer">
    SELECT * FROM mysqldata where mysqldataid=#{sourceid}
    </select>
</mapper>
